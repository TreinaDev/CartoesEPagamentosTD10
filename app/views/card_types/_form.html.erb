 <%= form_with model: @card_type, html:{class:'container'} do |f| %>
  
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, :class => 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :icon %>
      <%= f.text_field :icon, :class => 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :start_points %>
      <%= f.number_field :start_points, :class => 'form-control' %>
    </div>
  
    <%= f.submit 'Salvar', :class => 'btn btn-primary mt-1' %>
  
  <% end %>