<%= form_with model: @card_type, class: 'form' do |f| %>
  
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :icon %>
    <%= f.text_field :icon %>
  </div>

  <div>
    <%= f.label :start_points %>
    <%= f.number_field :start_points %>
  </div>

  <%= f.submit 'Salvar', :class => 'btn btn-primary mt-1' %>
  
<% end %>